{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-unused-variable": true,
        "object-literal-sort-keys": false,
        "quotemark": [ true, "single", "jsx-double" ],
        "member-access": [
            false
        ],
        "ordered-imports": [
            false
        ],
        "no-console": [
            false
        ],
        "no-var-requires": false
    },
    "rulesDirectory": []
}